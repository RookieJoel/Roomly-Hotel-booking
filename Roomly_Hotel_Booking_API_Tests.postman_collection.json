				{
					"name": "Register User (Success)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200 or 201\", function () {",
									"    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
									"});",
									"",
									"pm.test(\"Response has success=true\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.success).to.eql(true);",
									"});",
									"",
									"pm.test(\"Response contains token\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.token).to.exist;",
									"    pm.collectionVariables.set(\"user_token\", jsonData.token);",
									"});",
									"",
									"pm.test(\"User has required fields: name, tel, email\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.name || jsonData.data?.name).to.exist;",
									"    pm.expect(jsonData.email || jsonData.data?.email).to.exist;",
									"    pm.expect(jsonData.tel || jsonData.data?.tel).to.exist;",
									"});",
									"",
									"pm.test(\"✅ REQ 1: User can register with name, tel, email, password\", function () {",
									"    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
									"});"
